package dpack;
public class Numbers{
    public static void main(String[] args) {
        for (int i=1;i<=5;i++) {
            //Numbers
            for (int j=1;j<=i;j++) {
                if ( j%2==0) {
                    System.out.println(0);

                }else {
                    System.out.println(1);
                }
            }
            System.out.println();
        }
    }
}
